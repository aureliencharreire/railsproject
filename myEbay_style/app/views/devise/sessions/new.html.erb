
<div class="wrapper">
    <div class="container">
        <h1>Log in</h1>
        <div class="form">
            <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
            <input <%= f.email_field :email, autofocus: true, :placeholder => "Email" %>
            <input <%= f.password_field :password, autocomplete: "off", :placeholder => "Password" %>
            <% if devise_mapping.rememberable? -%>
            <div>
                <%= f.label :remember_me %>
                <%= f.check_box :remember_me %>
            </div>
            <% end -%>
            <button <%= f.submit%>Log in</button>
            <% end %>

        </div>
    </div>
    <ul class="bg-bubbles">
        <li></li>
        <li></li>
        <li></li>
        <li></li>
        <li></li>
        <li></li>
        <li></li>
        <li></li>
        <li></li>
        <li></li>
    </ul>
</div>

<script src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>
<script src="./app/assets_custom/JavaScript/js/logform.js"></script>

<%= render "devise/shared/links" %>
